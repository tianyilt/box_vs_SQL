int a[10],b[10],i;for(i=0;i<10;i++)   printf("a[%d]=%d,b[%d]=%d\n",i,a[i],i,b[i]);scanf("%d",&b[10]);printf("a[0]=%d",a[0]);
};2011/11/24 14:04:00;大学之学;;;;

func 函数内部循环条件应加上一个等于#include<stdio.h>#define N 11int func(int a,int b);void  main(){int i,k,j,a[N][N];for(i=1;i<N;i++){a[i][i]=1;a[i][1]=1;} for(i=3;i<N;i++)   {    for(j=2;j<=i-1;j++)    a[i][j]=func(i,j); }for(i=1;i<N;i++){ for(k=1;k<(N-i);k++)  printf("   ");for(j=1;j<=i;j++)  printf("%6d",a[i][j]);printf("\n");}printf("\n");}int func(int a,int b){ int q,n=1,w=1,e=1,sum; for(q=1;q<a;q++)      w*=q;  for(q=1;q<b;q++)      e*=q;   for(q=1;q<=(a-b);q++)          n*=q;   sum=w/(e*n);return(sum);}

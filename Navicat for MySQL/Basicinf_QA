#include <stdio.h>#include <time.h>
int rcall ( int x );
void main ( ){    unsigned long stuNumber[20][150];    char stuName[20][150][8];
    unsigned long *pNum;    char *pName;
    int call[150];    int count;    char ans;
    int i,j;           //循环用变量    int m,n[5];        //变量，表示班级以及人数    int k,l;           //其他变量，k用于函数返回
    pNum=&stuNumber[0][0];    pName=&stuName[0][0];
    printf("*****************************************\n");    printf("           随机点名程序V4.0版\n");    printf("*****************************************\n");
    printf("下面是输入部分： \n");    printf("请输入班级的数量。\n");
    scanf("%d",&m);    for(i=0;i<m;i++)    {        printf("请输入第%d个班级的人数\n",i+1);        scanf("%d",&n[i]);    }    for(i=0;i<m;i++)    {        printf("请输入第%d个班级的%d位学生的学号、姓名(先输入学号，再输入姓名)\n",i+1,n[i]);        for(j=0;j<n[i];j++)        {            scanf("%d",&stuNumber[i][j]);            scanf("%s",&stuName[i][j]);        }    }
    printf("现在开始点名，请输入您所在的班级号：\n");    scanf("%d",&m);    printf("输入成功，点名开始。\n");    printf("--------------------------------------\n");
    l=1,count=0;
    while(l==1)    //死循环，由用户决定是否退出（调试后发现非问题所在）    {        k=rcall(n[m]);//进行随机点名并把值赋给k，此时k取0到班级人数之间的值        if (count==0)   //判断是否为第一次点名，若非第一次，则需要验证是否重复        {            call[count]=k;//将每次判断的k的值输入进数组中，之后用于比较            count +=1;            printf("你点到的人的学号是：%d ，名字是：%c  ，恭喜这位同学！\n",*(pNum+(m-1)*150+k),*(pName+(m-1)*150+k));//输出对应的姓名学号            printf("是否再进行一次点名？请输入Y或N（大写）\n");            scanf("%c",&ans);            if(ans=='Y')     //判断用户输入的值并作进一步运行                l=1;            else            {                if(ans=='N')                    l=0;                else                {                    printf("输入格式错误！程序结束！");                    l=0;                }//else            }//else        }//if        else             //当不是第一次点名时        {            for (i=0;i<count;j++)   //进行循环判断是否点过名            {                if(call[i]==k)   //点过这个人时                {                    printf("你点过了这个人。。=____=\n");                    printf("是否重新点名？请输入Y或N（大写）\n");                    scanf("%c",&ans);                    if(ans=='Y')                        l=1;                    else                    {                        if(ans=='N')                            l=0;                        else                        {                            printf("输入格式错误！程序结束！\n\n");                            l=0;                        }//else                    }//else                }//if                else   //这是之前没点过的人时                {                    call[count]=k;                    count +=1;                    printf("你点到的人的学号是：%d ，名字是：%c  ，恭喜这位同学！\n",*(pNum+(m-1)*150+k),*(pName+(m-1)*150+k));                    printf("是否再进行一次点名？请输入Y或N（大写）\n");                    scanf("%c",&ans);                    if(ans=='Y')                        l=1;                    else                    {                        if(ans=='N')                            l=0;                        else                        {                            printf("输入格式错误！程序结束！\n\n");                            l=0;                        }//else                    }//else                }//else            }//for        }//else    }//while    printf("本次点名已结束，欢迎再次使用点名程序。\n");    printf("谢谢使用，再见！\n\n");    printf("**********************************************************\n");    printf("    由于技术原因需要手动输入名单，之后的版本将会改进。\n");    printf("       使用过程中如遇到问题请反馈，我们会及时处理\n");    printf("                         created by byj\n");    printf("**********************************************************\n");}
int rcall( int x ){    int s;    s=rand()%x+0;    return s;}

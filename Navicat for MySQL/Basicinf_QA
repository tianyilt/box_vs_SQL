#include<stdio.h>#include<math.h>void main(){float a,b,c;double m,x1,x2;printf("这是一个一元二次方程ax^2+bx+c=0\n");printf("请输入系数a：");scanf("%f",&a);printf("请输入系数b：");scanf("%f",&b);printf("请输入系数c：");scanf("%f",&c);m=pow(b,2)-4*a*c;if (m>=0)if (m>0){x1=(-b+sqrt(m))/2;x2=(-b-sqrt(m))/2;printf("x1=%lf,x2=%lf",x1,x2);}else {x1=-b/2;printf("x1=x2=%lf",x1);}else{x1=(-b+sqrt(-m))/2;x2=(-b-sqrt(-m))/2;printf("x1=%lf\i,x2=%lf\i",x1,x2);}}

~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~分割线
double zero(double a,double b,double(*f)(),double eps){ double c,x; do { c=(a+b)/2;  x=(*f)(a)*(*f)(c);  if(x<0)   b=c;  else if(x>0)   a=c;  else break; }while(fabs(a-b)>eps); return c;}double fun(double x){ return x*x+x-2;}#include<stdio.h>main(){ int a=-3,b=-1,c=3,d=4; double sin(),fun(); printf("x^2+x-2 is %.2f\nsin(x) is %.2f\n",zero(a,b,fun,0.01),zero(c,d,sin,0.01));}

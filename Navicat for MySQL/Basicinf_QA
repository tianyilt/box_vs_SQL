void change (int *pi[]){ int *pmax,*pmin,m,t;
 *pmax=*pi[0]; *pmin=*pi[0]; for (m=1;m<3;m++) {  if (*pi[m]<*pmin)   *pmin=*pi[m];  if (*pi[m]>*pmax)   *pmax=*pi[m]; }

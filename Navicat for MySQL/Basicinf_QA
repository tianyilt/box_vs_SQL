标 } }void readslist()//读取名单文件的函数{   int i;   FILE *fp1;   fp1=fopen("d:\\C语言程序设计\\实验报告\\slist.txt","rt");//此处目录应根据需要进行修改   if (fp1==NULL)   {     printf("文件打开失败！\n");     exit(-1);   }   for(i=0;i<500;i++)   {     fgets(slist[i],23,fp1);      if (strcmp(slist[i],"")==0)   break;   }//将学生学号姓名逐行读入字符串数组中   fclose(fp1);   n=i;}V5#include <stdio.h>#include <stdlib.h>#include <conio.h>#include <time.h>#include <malloc.h>void main(){char ch[300][40];int i,num;int k,*p;FILE *fp1;fp1=fopen("list.txt","r"); if(fp1==NULL){printf("Error!");exit(-1);}   for(i=0;!(feof(fp1));i++)   fgets(ch[i],30,fp1);fclose(fp1);  printf("请输入你想抽取的人数：  ");scanf("%d",&num);    while(1)    {  if(num>=i)  {printf("输入的人数太多！重新输入：");scanf("%d",&num);}  else break; }  p=(int*)malloc(num*sizeof(int));p[0]=-1;srand(time(0));  for(k=0;k<num;k++)       while(p[k]<0||p[k]>=i)   p[k]=rand();printf("随机抽到的人为：\n");      for(k=0;k<num;k++)printf("%s",ch[p[k]]);free(p);}执行不出效果，哪里出错了呢？

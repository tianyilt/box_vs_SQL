void main(){    int a,b,c; double d,x1,x2; 
 printf("please input a:"); scanf("%d",&a);
 printf("please input b:"); scanf("%d",&b);
    printf("please input c:"); scanf("%d",&c);
 d=b*b-4*a*c; if (d>=0) {  d=sqrt(d);     x1=(-b-d)/(2*a);        x2=(-b+d)/(2*a);     printf("x1=%f,x2=%f\n",x1,x2); } else if (d<0)  printf("对不起,方程没有实根,本程序解不开啊");}需要ELSE IF 么。。。

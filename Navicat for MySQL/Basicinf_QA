1074;1463;HuQingchun;https://c.rin.to/course/mod/forum/discuss.php?d=1463;来自2位同学的实验三和实验五的答案供大家参考;贴在此处的实验三和实验五的答案，仅供大家参考。;#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(void)
{
    float a, b, c, cir, hcir, area;

    printf("请输入三条边长，以英文逗号隔开：\n");
    scanf("%f, %f, %f", &a, &b, &c);    //输入三条边长

    cir = a + b + c;    //计算三角形周长
    hcir = cir/2;   //半周长
    area = pow( (hcir*(hcir-a)*(hcir-b)*(hcir-c)), 0.5 );     //计算三角形面积（海伦公式）

    printf("cir=%f, area=%f\n", cir, area);


    return 0;
}#include <stdio.h>#include <stdlib.h>int main(void){    char a, b;    printf("请输入一个小写字母：");    a = getchar();      //使用字符输入函数得到一个小写字母    putchar('\n');      //光标换行    b = a - 32;         //将小写字母转换为大写字母    printf("转换后的大写字母为：");    putchar(b);    putchar('\n');      //光标换行    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    int a, b, temp;    printf("请输入a, b的值, 以英文逗号隔开:");    scanf("%d, %d", &a, &b);    temp = a;    a = b;    b = temp;    printf("交换后a, b 的值为:");    printf("a=%d,b=%d\n",a ,b);    return 0;}#include <stdio.h>#include <stdlib.h>void sum(float a, float b){    printf("sum=%f\n", a + b);}void sub(float a, float b){    printf("sub=%f\n", a - b);}void mul(float a, float b){    printf("mul=%f\n", a * b);}void divd(float a, float b){    printf("div=%f\n", a / b);}int main(void){    float a, b;    printf("请输入两个非零数字，一英文逗号隔开:\n");    scanf("%f, %f", &a, &b);    sum(a, b);    sub(a, b);    mul(a, b);    divd(a, b);    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    float nScore, sScore, bScore, sum;    printf("欢迎进入成绩管理系统！\n");    printf("系统开发人员：** **！\n");    printf("系统版本信息：第一版！\n");    printf("请以此输入考勤分数,小作业分数,大作业分数,以英文逗号隔开:\n");    scanf("%f, %f, %f", &nScore, &sScore, &bScore);    sum = nScore + sScore + bScore;    printf("平时成绩总分：%.f\n", sum);    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    float a, b;     //存放两个数的变量    int tag  = 0;   //运算合法的标志，0--合法，1--非法    char ch;        //运算符变量    float result;   //运算结果变量    printf("input two number: ");   //提示输入两个数    scanf("%f%f", &a, &b);          //输入两个数    fflush(stdin);                  //清键盘缓冲区    printf("input arithmetic lable(+-*/): ");    //提示输入运算符    scanf("%c", &ch);               //输入运算符    switch(ch)      //根据运算符来进行相关的运算    {        case '+' : result = a + b; break; //加法运算        case '-' : result = a - b; break; //减法运算        case '*' : result = a * b; break; //乘法运算        case '/' :            if(!b)            {                printf("divisor is zero!\n");   //显示除数为0                tag = 1;        //置运算非法标志            }            else            {            result = a / b;  //除法运算            }            break;        default : printf("illegal arithmetic lable\n");     //非法的运算符                    tag = 1;    //置运算非法标志    }    if( !tag )      //运算合法，显示运算结果    {        printf( "%.2f %c %.2f=%.2f\n", a, ch, b, result);    }    return 0;}#include <stdio.h>#include <stdlib.h>#include <math.h>int main(void){    float a, b, c, cir, hcir, area;    printf("请输入三角形的三条边长：以英文逗号隔开：\n");    scanf("%f,%f,%f", &a, &b, &c);    if(a&&b&&c) //三条边均为正数        if(a+b>c && a+c+b && b+c>a)        {            cir = a + b + c;            hcir = cir/2;            area = sqrt(hcir*(hcir-a)*(hcir-b)*(hcir-c));            printf("cir=%.2f, area=%.2f\n", cir, area);        }        else            printf("不能组成三角形！\n");    else        printf("三角形的边长必须为正数！\n");    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    int num, a, b, c;    printf("请输入一个三位数：\n");    scanf("%d", &num);    a = num/100;    b = (num-100)/10;    c = num%10;    if(a*a*a + b*b*b + c*c*c == num)        printf("%d是一个水仙花数\n", num);    else        printf("%d不是一个水仙花数\n", num);    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    int num;    printf("请输入一个五分制成绩：\n");    scanf("%d", &num);    switch(num)    {        case 5 : printf("成绩为A类\n"); break;        case 4 : printf("成绩为B类\n"); break;        case 3 : printf("成绩为C类\n"); break;        default : printf("成绩为D类\n"); break;    }    return 0;}#include <stdio.h>#include <stdlib.h>float max(float x, float y){    float t;    if(x < y)    {        t = x;        x = y;        y = t;    }    return x;}int main(void){    float a, b, c;    printf("请输入三个数字，以英文逗号隔开：\n");    scanf("%f,%f,%f", &a, &b, &c);    printf("max=%f\n",  max(a, max(b, c)));    return 0;}#include <stdio.h>#include <stdlib.h>int main(void){    float nScore, sScore, bScore;    printf("欢迎进入成绩管理系统！\n");    printf("系统开发人员：**  **！\n");    prin
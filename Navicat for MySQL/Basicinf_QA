void main(){int i,j;float score[6][6]={{80,90,75,82},{90,75,88,95},{68,78,88,82},{72,85,63,86},{95,88,90,85}};char name[6][10]={"李一","成红","王强","张一","席二"," "};for (i=0;i<5;i++){  for (j=0;j<4;j++)  {  score[i][4]+=score[i][j];     score[5][j]+=score[i][j];  }  score[i][5]=score[i][4]/=4;}
  for (j=0;j<4;j++)  score[5][j]/=5;

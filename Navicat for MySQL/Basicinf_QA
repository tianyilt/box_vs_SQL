1771;3035;瞿驰远;https://c.rin.to/course/mod/forum/discuss.php?d=3035;可以控制个数，格式，打印菱形的程序;
#include <stdio.h>
int main()
{
    int A,B,C,a,c,i,L,l,n;//A为单个菱形尺寸，B为菱形总数量，C为每行菱形数量，ac分别用于计数，i用于菱形内部计数,n为单位菱形每行空格数量,L为菱形行数,l用于总行数的循环,ne为最后一行菱形个数
    printf("请输入以下几个参数并用逗号分隔:单个菱形尺寸，菱形总数，每行菱形的数量");
    scanf("%d,%d,%d",&A,&B,&C);
    if(C>B)
    {
        printf("您输入的数量有误，每行菱形数量不可能大于总数量，请重新输入：");
        scanf("%d,%d,%d",&A,&B,&C);
    }
    if(B%C==0)//判断总行数
    {
        L=B/C;
        n=C;
    }
    else
    {
        L=B/C+1;
        n=B%C;
    }
    for(l=1;l<=L-1;l++)//菱形行循环,除最后一行
    {
        for(a=1;a<=A;a++)//单行菱形内部行循环,打印上半个菱形
        {
            for(c=1;c<=C;c++)//行内打印菱形循环
            {
                for(i=a;i<=A;i++)
                    printf(" ");
                for(i=2*a-1;i>=1;i--)
                    printf("*");
                for(i=a;i<=A;i++)
                    printf(" ");
            }
            printf("\n");
        }
        
        
        for(a=1;a<=A;a++)//单行菱形内部行循环，打印下半个菱形
        {
            for(c=1;c<=C;c++)//行内打印菱形循环
            {
                for(i=a+1;i>=1;i--)
                    printf(" ");
                for(i=2*a-1;i<=2*A-3;i++)
                    printf("*");
                for(i=a+1;i>=1;i--)
                    printf(" ");
            }
            printf("\n");
        }
    }
    
    for(l=1;l<=1;l++)//菱形行循环,对最后一行单独处理
    {
        for(a=1;a<=A;a++)//单行菱形内部行循环,打印上半个菱形
        {
            for(c=1;c<=n;c++)//行内打印菱形循环
            {
                for(i=a;i<=A;i++)
                    printf(" ");
                for(i=2*a-1;i>=1;i--)
                    printf("*");
                for(i=a;i<=A;i++)
                    printf(" ");
            }
            printf("\n");
        }
        
        
        for(a=1;a<=A;a++)//单行菱形内部行循环，打印下半个菱形
        {
            for(c=1;c<=n;c++)//行内打印菱形循环
            {
                for(i=a+1;i>=1;i--)
                    printf(" ");
                for(i=2*a-1;i<=2*A-3;i++)
                    printf("*");
                for(i=a+1;i>=1;i--)
                    printf(" ");
            }
            printf("\n");
        }
    }
    return 0;
}
文件因为编码格式不同会有乱码的问题，可以直接看以上文本

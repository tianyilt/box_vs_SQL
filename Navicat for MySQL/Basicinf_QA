#include<stdio.h>void main(){ int a[6][6],i,j,k; for(i=0;i<6;i++) {  a[i][0]=1;  a[i][i]=1;  for(k=1;k<i+1;k++)   a[i+1][k]=a[i][k-1]+a[i][k];  for(j=0;j<20-2*i;j++)   printf(" ");  for(j=0;j<i+1;j++)   printf(" %2d ",a[i][j]);  printf("\n");   }}
 
 
 
 
#include<stdio.h>void main(){ int a[6][6],i,j,k; for(i=0;i<6;i++) {  a[i][0]=1;  a[i][i]=1;  for(k=1;k<i;k++)   a[i][k]=a[i-1][k-1]+a[i-1][k];  for(j=0;j<20-2*i;j++)   printf(" ");  for(j=0;j<i+1;j++)   printf(" %2d ",a[i][j]);  printf("\n");   }};第一个，for(i=0;i<6;i++)，i最大可以达到5，for(k=1;k<i+1;k++)，k最大可以达到5，a[i+1][k]会读写a[6][5]，范围越界数组访问越界在C语言里不是错误，但是程序行为不确定嗯，真的是这样，我把数组改成a[7][7]就没有再出现错误

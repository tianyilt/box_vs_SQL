 if (x<0) c=-1; if (0<=x&&x<10) c=0; if (10<=x&&x<20) c=1; if (20<=x&&x<40) c=2; switch(c) { case -1:y=0;break; case 0:y=x;break; case 1:y=10;break; case 2:y=20-0.5*x;break; default:printf("y=%f",y); }

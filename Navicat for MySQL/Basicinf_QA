#include <stdio.h>void main(){ char a,b,c; printf("现在我们来进行一个性格测验\n"); printf("你喜欢吃苹果吗？(Y/N)\n"); scanf("%c",&a); if (a=='Y') {  printf("你酷爱运动吗？(Y/N)\n");  scanf("%c",&b);  if (b=='Y')  {   printf("你会游泳吗？(Y/N)\n");   scanf("%c",&c);   if (c=='Y')    printf("你是一个友善的人\n");   else    printf("你是一个不擅言辞的人\n");  }else  {   printf("你喜欢唱歌吗？(Y/N)\n");   scanf("%c",&c);   if (c=='Y')    printf("你是一个热爱和平的人\n");   else    printf("你是一个脾气暴躁的人\n");  } }else {  printf("你喜欢电子竞技吗？(Y/N)\n");  scanf("%c",&b);  if (b=='Y')  {   printf("你去过游乐园吗？(Y/N)\n");   scanf("%c",&c);   if (c=='Y')    printf("你是一个开朗的人\n");   else    printf("你是一个内向的人\n");  }else  {   printf("你酷爱运动吗？(Y/N)\n");   scanf("%c",&c);   if (c=='Y')    printf("你是一个乐观的人\n");   else    printf("你是一个悲观的人\n");  } }}

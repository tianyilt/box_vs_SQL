r(reply)=='Y');        //实现程序多次循环使用 直到用户选择退出}void check_repeat(int *used,int maxnumber,int *random){ int i; for(i=0;i<maxnumber;i++) {  if(*random==used[i])  {   *random=rand()%(maxnumber+1);   check_repeat(used,maxnumber,random);            //用递归实现反复确认是否重复  }  continue; }}void Free_LinkList(STU *head){ STU *p,*q; p=head; while(p->next!=NULL) {  q=p->next;  p->next=q->next;  free(q); } free(head);}#include <stdio.h>void main(){    int i,j,a,b;    printf("请输入第一名同学的学号\n");    scanf("%d",&a);    printf("请输入最后一名同学的学号\n");    scanf("%d",&b);    for (i=0;;i++)                //通过死循环让屏幕上一直出现滚动的学号 直到老师按pause暂停        for(j=a;j<b;j++)            printf("%d\n",j);}#include <stdio.h>#include <stdlib.h>#include <time.h>int main(){int jud,num;int x,y;printf("1.点名 2.结束点名\n");scanf("%d",&jud);while(jud==1){printf("请输入你想点名的学号范围，比如10到100：");scanf("%d到%d",&x,&y);srand((unsigned)time(NULL));num=rand()%(y-x+1)+x;if(num<10)printf("抽到的学号为：1014000%d",num);else if(num<100)printf("抽到的学号为：101400%d",num);else if(num<1000)printf("抽到的学号为：10140%d",num);else if(num>999)printf("抽到的学号为：10140%d",num);printf("\n");printf("1.点名 2.结束点名\n");    scanf("%d",&jud);   }}srand种子充值需要放到循环外面，不然出现的还是尾伪随机数#include <stdio.h>#include <stdlib.h>#include <time.h>int main(){int jud,num;int x,y;printf("1.点名 2.结束点名\n");scanf("%d",&jud);printf("请输入你想点名的学号范围，比如输入'10到100'：");scanf("%d到%d",&x,&y);srand((unsigned)time(NULL));while(jud==1){num=rand()%(y-x+1)+x;if(num<10)printf("抽到的学号为：1014000%d",num);else if(num<100)printf("抽到的学号为：101400%d",num);else if(num<1000)printf("抽到的学号为：10140%d",num);else if(num>999)printf("抽到的学号为：10140%d",num);printf("\n");printf("1.点名 2.结束点名\n");    scanf("%d",&jud);    }}#include

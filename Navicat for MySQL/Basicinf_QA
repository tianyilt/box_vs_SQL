Debug-1.C
#include <stdio.h>#include <math.h>void main(){    double x1;    double x2;    double a,b,c,d; printf("please input a,b,c\n"); scanf("%f%f%f\n",&a,&b,&c); printf("a=%f,b=%f,c=%f\n",a,b,c); d=b*b-4*a*c; printf("%d\n",d);  if   (d<0)  printf("No answer"); else        x1=(-b+sqrt(d))/(2*a);     x2=(-b-sqrt(d))/(2*a);        printf("%lf\n",x1,x2);

}

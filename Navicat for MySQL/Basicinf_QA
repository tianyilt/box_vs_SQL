#include <stdio.h>
 
void main ()
{
        int
k[6];
        int
i, j, sum, tag=1;
 
        for
(j=0; j<21; j++)
        {
                 sum=0;

                 k[0]=j;
                 for
(i=1; i<=6; i++)
                 {
                         k[i]=k[i-1]+i;
                         if
(k[i]>36)
                         {
                                  tag=0;
                                  break;
                         }
                 }
                 if
(tag==0)
                         continue;
                 for
(i=0; i<=6; i++)
                 {
                         sum=sum+
k[i];
                 }
                 if
(sum==105)
                 {
                         printf
("7个号码分别为：");
                         for
(i=0; i<=5; i++)
                                  printf
("%d ", k[i]);
                         break;
                 }
                 printf
("j=%d\n",j);
        }

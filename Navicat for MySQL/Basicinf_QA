{ int m,n,i;
printf("请输入两个正整数m和n；");
scanf("%d,%d",&m,&n);
if(m<=n)
    for(i=m;i<=m&&i>=0;i=i-1)
    {
        if(m%i==0&&n%i==0)
        printf("最大公约数为%d",i);
        break;

    }
    else
         for(i=n;i<=n&&i>=0;i=i-1)
         {
        if(m%i==0&&n%i==0)
        printf("最大公约数为%d",i);
        break;
    }

    return 0;
};2018/10/8 13:14:00;大学之学;;;;

 printf("please input %d number:\n",num); for (i=0;i<num;i++) scanf("%d",&a[i]);
  for (i=1;i<num;i++) {  flag=0; for (j=0;j<num-i;j++)  if (a[j]>a[j+1]) { t=a[j];     a[j]=a[j+1];     a[j+1]=t;  flag=1;} if (flag==0) break; }  printf("the sorted numbers:\n"); for (i=0;i<num;i++)  printf("%d",a[i]);
}


#include <stdio.h>#include <math.h>
int fun (char b[]);void main (){ int n, i; char b[30]={0}; n= fun (b); printf ("满足条件的整数的个数为%d\n",n); for (i=0; b[i]!='\0'; i++)  printf ("%d ",b[i]);}
int fun (char a[]){ int x, n=0, hundred, decade, unit; double root;
 for (x=100;x<=999;x++) {  root=sqrt(x);  if ((int)root==root)  {   hundred=x/100;   decade=(x/10)%10;   unit=x%10;   if (hundred==decade || hundred==unit || decade==unit)   {    a[n]=x;    n++;   }  } } return n;}﻿
 
 ;天哪！我知道了！！数组类型定义成char了，应该改成int。
 

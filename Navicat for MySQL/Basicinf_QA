#include <stdio.h>
#include <stdio.h>
#include <stdlib.h>
#include <time.h> 
#define tot 30
 
struct student
{
       char no[9]
;
       char
name[21] ;
};
 
void main ()
{
int i;
struct student stu[tot] ;
int number ;
FILE *fp ;
char answer ;
 
printf ("姓名：吴思玥\n");
printf ("学号：10152965\n");
printf ("实验报告3-1-V5\n");
printf ("随机点名V5（文件）\n");
 
printf ("请将名单命名为
10152965V5student.txt 文件内包含学号与姓名\n")
;
printf ("学生总人数：%d人\n",tot);
 
fp=fopen
("10152965V5student.txt","rt") ;
if (fp==NULL)
{
       printf
("cannot open file\n") ;
       printf
("请将名单命名为 10152965V5student.txt 文件内包含学号与姓名\n")
;
       exit(-1);
}
 
for (i=1 ; i<=tot ; i++)
       fscanf(fp,"%s%s",stu[i].no,stu[i].name)
;
 
while (answer!='N')
{
       srand((unsigned)
time(NULL)); 
       number =
rand() % (tot+1);
       printf
("\n  学 号     姓名\n")
;
       printf("%s    %s \n",stu[number].no,stu[number].name);
       printf
("输入N结束点名,否则任意键继续:
") ;
       fflush
(stdin) ;
       scanf
("%c",&answer) ;
}
 
fclose(fp);
 
}
 

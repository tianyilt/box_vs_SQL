#include<stdio.h>#include<stdlib.h>#include<time.h>void main(){ int x,y;int s; printf("请输入点名班级的同学起始学号：");  scanf("%d",&x); printf("请输入点名班级的同学最后学号："); scanf("%d",&y); s=y-x+1; srand ((int)time(0)); int n;int i; printf("请输入点名的次数:"); scanf("%d",&n); for(i=0;i<n;i++)  printf("%d\n",rand()%s+x);}版本v1的修改
#include<stdio.h>#include<stdlib.h>#include<time.h>void main(){ int x,y;int s;int a; printf("请输入点名班级的同学起始学号：");  scanf("%d",&x); printf("请输入点名班级的同学最后学号："); scanf("%d",&y); s=y-x+1; srand ((int)time(0)); while(1)  { printf("幸运同学的学号为:");  printf("%d\n",rand()%s+x);  printf("请问是否继续点名呢？\n如果是的话请输入1，否则请输入0!");  scanf("%d",&a);  if(a==1)   continue;  else   break;} printf("谢谢使用!此次点名结束！");}
 看了形形色色的点名程序，我觉得如果使用文件的话主要就是一个问题  如果用结构体 那势必要先建立一个严格符合结构体的txt文件我觉得如果真的有人要去点名   他绝对不会用一个第一次使用的时候还要把所有人名字学号输入一遍的程序估计也不会有人去建立一个数据库存这些名单程序是减少人的负担的，不是给人添麻烦的。而且每一个老师是一定有一份教务处传过来的excel名单的 那我就想 名单直接copy到txt文本里这个时候一行就是一个学生的信息点名程序  只要随机移动文件指针 然后提取相邻的两个回车符之间的内容打印出来即可所以我设计了下面这个程序。没有考虑打不开文件等一系列问题 只是写下了核心部件最大的问题在运行的时候大家就能发现因为文件指针位置随机 当点名程序恰巧点到最后一个人的时候这个数据就莫名其妙的溢出了原因很简单  这组数据中只有一个回车和程序设计的基本路线 即"取两个回车之间的数据”相左这个指针似乎越过文件的结尾 跳到了文件的其他位置 导致输出的结果混乱甚至会发生难以预料的崩溃。讽刺的是 这个崩溃也是随机的。 但是并不大影响整个点名程序的进程   他依旧可以继续进行 严格的说对名单上最后一名同学的点名 这个程序也是成功的只是发生了错误，导致输出了多余的数据而修缮这个错误还是比较复杂的  所以我偷了个懒既然不是致命的bug而且发生的几率在样本容量大的时候也不算太高姑且先那么放着丢脸吧！txt文本的样本  我放一个在附件里  源代码如下 喜欢的同学可以试着改改#include <conio.h>#include <stdio.h>#include <stdlib.h> #include <time.h> #include <string.h>void check(int mem);void main(){FILE *fp;fpos_t pos;char s[150]={'\0'},a[150]={'\0'},b[160]={'\0'};    int mem,t;long f1,f2,f;int d=0,i,i1,i2;    srand(time(NULL));    //种子printf("*************************\n");printf("*  随机点名程序欢迎你！ *\n");    printf("*************************\n");    printf("*  Version 5.0 By Jhf   *\n");    printf("\n\n");printf("请将教务处提供的花名册拷贝到根目录下,并命名为 name.txt\n  press enter to continue\n");  //本程序基于学生名单运行getchar();fp=fopen("name.txt","r");if(fp==NULL){printf("WARNING: cannot find name.txt\n版本将降级为V4版本，请谅解!\n");//没有名单自动降级printf("班级里有几名学生？: ");scanf("%d",&mem);printf("你打算点几次？：");scanf("%d",&t);    do{check(mem);  t=t-1;}while(t!=0);exit(0);}L:printf("点名程序V5已经就位,press enter to continue");   fflush(stdin);  getchar(); //这句话没有实际意义fp=fopen("name.txt","r");/*while(fgetc(fp)!=-1)       这种方法没有成功  总是残缺不全的数据{l1=ftell(fp);fread(ch,1,20,fp);l2=ftell(fp);for(i2=0;i2<20;i2++){if(ch[i2]=='\n')t++;if (t==r){pos=l2-l1+i2-6;fsetpos(fp,&pos);fread(a,1,20,fp);goto L;}*/f1=ftell(fp);fseek(fp,0,SEEK_END);f2=ftell(fp);f=f2-f1;//计算文件长度fclose(fp);pos=rand()%f+1;      fp=fopen("name.txt","r");  fsetpos(fp,&pos); //移动指针到随机位置fread(a,1,70,fp);   //从随机的位置开始读数据i--;fclose(fp);for(i2=0;a[i2]!='\n';i2++)  //去掉数组a第一个回车之前的不完整数据;i2++;for(i1=0;a[i2]!='\n';i1++,i2++)//   将两个回车之间的完整信息存入b{b[i1]=a[i2];printf("%c",b[i1]);}strcpy(a,s);strcpy(b,s);   //重置printf("按1继续点名\n");fflush(stdin);    scanf("%d",&i);if(i==1)        //询问用户goto L;}void check(int mem){    int r;r=rand()%mem+1;printf("%d 号同学\n",r);}
#include
<stdio.h>
#include
<time.h>
#include<stdlib.h>
void
main()
{
int i,lucky;
long int nummin,nummax;
srand((int)time(NULL));
printf("请输入一个学号范围，必须包含所有学号，中间可以有无效学号：\n");
printf("起始学号：");
scanf("%d",&nummin);
fflush(stdin);
printf("终止学号：");
scanf("%d",&nummax);
mark:
lucky=rand()%(nummax-nummin+1)+nummin;
printf("%d\n",lucky);
printf("是否有这个同学，如果有，输入 1，没有输入 0：");
scanf("%d",&i);
if(i==0)
goto mark;

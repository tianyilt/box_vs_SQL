void Sort(int a[],int n);
int main(int argc, const char * argv[]) {
    int a[1000];
    int i=0,j;
    printf("Please input an array:");
    do
    {
        scanf("%d",&a[i]);
        i++;
    }
    while(getchar()!='\n');
        Sort(a,i);
        for(j=0;j<i;j++){
            printf("%3d",a[j]);
        }
       printf("\n");
    return 0;
}
void Sort(int a[],int n){
    int i;
    int j;
    int temp = 0;
    for(i=0;i<n;i++){
        for(j=0;j<n-1;j++){
        if (a[j]<a[j+1])
            temp = a[j];
            a[j] = a[j+1];
            a[j+1] = temp;
        }
    }
}
;#include <stdio.h>
void Sort(int a[],int n);
int main(int argc, const char * argv[]) {
    int a[1000];
    int i=0,j;
    printf("Please input an array:");
    do
    {
        scanf("%d",&a[i]);
        i++;
    }
    while(getchar()!='\n');
        Sort(a,i);
        printf("%d",a[0]);
        for(j=1;j<i;j++){
            printf(" %d",a[j]);
        }
        printf("\n");
    return 0;
}
void Sort(int a[],int n){
    int i;
    int j;
    int temp = 0;
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
        if (a[j]<a[j+1])
            temp = a[j];
            a[j] = a[j+1];
            a[j+1] = temp;
        }
    }
}


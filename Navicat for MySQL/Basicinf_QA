6,4);    strncpy(month,id+10,2);    strncpy(day,id+12,2);    printf("%s年%s月%s日",year,month,day);    return 0;}int main(){    char id[20];    printf("请输入您的身份证号码：");    gets(id);    printf("您的出生日期为：");    birth(id);    return 0;}#include <stdio.h>
#include <stdlib.h>
#define N 20
int main()
{
    FILE *fp1,*fp2;
    int i,j,k;
    char name[N],id[N],bdyear[N],bdmonth[N],bdday[N];
    fp1=fopen("idcard.txt","r");
    if(fp1==NULL)
    {
        puts("can't open idcard.txt");
        exit(0);
    }
    fp2=fopen("birthday.txt", "w");
    if(fp2==NULL)
    {
        puts("can't open birthday.txt");
        exit(0);
    }//检测文件是否存在
    while(!feof(fp1))//开始数据的输入，将生日分配到三个数组当中，并且去除月，日f的零
    {
        fgets(name, N, fp1);
        fgets(id, N, fp1);
        for(i=6,j=0;i<10;i++,j++)
            bdyear[j]=id[i];
        bdyear[j]='\0';
        for(i=10,j=0;i<12;i++,j++)
        {
            if('0'==id[i])//如果月有零，则跳过id此位
            {
                i++;
            }
            bdmonth[j]=id[i];
        }
        bdmonth[j]='\0';
        for(i=12,j=0;i<14;i++,j++)
        {
            if('0'==id[i])//如果日有零，则跳过此id位
            {
                i++;
            }
            bdday[j]=id[i];
        }
        bdday[j]='\0';
        //接下来开始输出
        fputs(name,fp2);//文件中输出
        fputc('\n',fp2);
        for(i=0;bdyear[i]!='\0';i++)
            fprintf(fp2,"%c",bdyear[i]);
        fprintf(fp2,"年");
        for(i=0;bdmonth[i]!='\0';i++)
            fprintf(fp2,"%c",bdmonth[i]);
        fprintf(fp2,"月");
        for(i=0;bdday[i]!='\0';i++)
            fprintf(fp2,"%c",bdday[i]);
        fprintf(fp2,"日");

        puts(name);//在exe中输出
        printf("\n");
        for(i=0;bdyear[i]!='\0';i++)//在exe中输出
            printf("%c",bdyear[i]);
        printf("年");
        for(i=0;bdmonth[i]!='\0';i++)
            printf("%c",bdmonth[i]);
        printf("月");
        for(i=0;bdday[i]!='\0';i++)
            printf("%c",bdday[i]);
        printf("日");
    }
    fclose(fp1);
    fclose(fp2);
    return 0;
}

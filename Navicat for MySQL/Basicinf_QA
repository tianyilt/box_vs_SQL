 c = x < 0 ? -1 : x / 10; switch (c)  {   case -1:  y =0;             break;   case 0:  y =x;            break;   case 1:  y =10;            break;   case 2:   case 3:   y = -0.5*x+20;            break;   default:  y = -2;  }if (y !=-2)   printf ("y = %f\n", y);else   printf ("error\n");}

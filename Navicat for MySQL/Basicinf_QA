void paixu(int a[]){int i,j;for(i=0;i<4;i++) for(j=i+1;j<5;j++)  if(a[i]>a[j])  {  a[i]=a[i]^a[j];  a[j]=a[i]^a[j];     a[i]=a[i]^a[j];  }}

ctangle((one-1)*200+(N-n)*7,390-(pan[one-1])*14-6,(one-1)*200+180-(N-n)*7,390-(pan[one-1])*14+6);   dizuo();   setlinestyle(PS_SOLID,NULL,12);    line((three-1)*200+(N-n)*7+14,390-(pan[three-1]-1)*14,(three-1)*200+166-(N-n)*7,390-(pan[three-1]-1)*14);}void hanoi(int n,int one,int two,int three){    if (n==1)  {        move(n-1,one,three);    }  else   {        hanoi(n-1,one,three,two);  Sleep(2000);        move(n-1,one,three);   Sleep(2000);        hanoi(n-1,two,one,three);  }} void main(){    initgraph(640,480);       setbkcolor(BLUE);   cleardevice();     drawtext(_T("汉诺塔动画演示"), &r, DT_CENTER | DT_VCENTER | DT_SINGLELINE);         outtextxy(300,140,"step:");   pan[0]=N;         dizuo();      hanoi_draw();     Sleep(2000);    hanoi(N,1,2,3);     getch();    closegraph();};2014/12/24 08:10:00;大学之学;;;;

 {  while (b=0)  {   scanf("%d",&a);      i++;      if(i<=10 && a<n)   {    b=-1;       printf("您输入的数%d过大，重新再来一次\n",a);   }      else if (i<=10 && a>n &&a<100)   {    b=1;       printf("您输入的数%d过小，重新再来一次\n",a);   }      else if (i>10)   {    b=0;       printf("对不起，您的10次机会已经用完。您失败了\n");    printf("是否继续游戏，是请按'Y'，否请 'N'：\n");   }      else if (a==n)   {    b=0;       printf("好厉害，你答对了，您一共用了%d\n",i);    printf("是否继续游戏，是请按'Y'，否请 'N'：\n");   }      else if (a>100)   {    b=0;       printf("您放弃了游戏,游戏结束");    printf("是否继续游戏，是请按'Y'，否请 'N'：\n");   }    }scanf("%c,&ch");  }while (ch=='N');

53;192;1011178510111785;https://c.rin.to/course/mod/forum/discuss.php?d=192;关于switch的问题;#include <stdio.h>void main (){ float x,y,z; printf("x="); scanf("%f",&x); if (x<0)  z='1'; else if(0<=x&&x<10)  z='2'; else if(10<=x&&x<20)  z='3'; else if(20<=x&&x<40);     z='4'; switch (z) { case'1':printf("y=0\n");break; case'2':printf("y=%f\n",x);break; case'3':printf("y=10\n");break; case'4':printf("y=%f\n",-0.5*x+20);break;}}

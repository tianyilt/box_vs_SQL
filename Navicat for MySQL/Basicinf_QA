//一个班上学号大多数是连续的，用循环在一个连续区间内抽取随机数。但是会出现重复现象#include<stdio.h>#include<time.h>#include<stdlib.h>#include<conio.h>void main(){    int i,j,max,min,m,n;    printf("版本V1.只能在连续学号内随机抽取。可能重复抽取。\n输入学号范围：(分别输入最小值和最大值)");    scanf("%d%d",&min,&max);    fflush(stdin);//防止不慎多输入导致人数错误            m=max-min+1;//计算总人数     srand(time(0)); //随机数种子 loop:    printf("输入点名人数：");    scanf("%d",&n);    fflush(stdin);    for(i=0;i<n;i++)    {        j=rand()%m+min;//计算学号         printf("学号为%d的同学\n",j);    }printf("是否继续输入 1-yes else-no:");    if(getch()=='1') {putchar('\n'); goto loop;}    else printf("\n谢谢使用\n");}//////////////////////////////////////////////////////////////V2////////////////////////////////////////////////////////////////////////////////////////////////////////  //相比上一个版本，加入了一个数组，避免的重复学号的现象，因此限制了点名人数#include<stdio.h>#include<time.h>#include<stdlib.h>#include<conio.h>void main(){    int i,j,max,min,m=0,n,t[100],s=0;//数组存放已被选到的学生，大小足够大，s为判断时所用    printf("版本V2.只能在连续学号内随机抽取。\n输入学号范围：(分别输入最小值和最大值)");    while(1)//错误输入时重新输入    {    scanf("%d%d",&min,&max);    fflush(stdin);    m=max-min+1;     if(m>0) break;    else printf("输入错误，请重新输入.\n");    }i=0;loop:    while(1)//限制点名人数    {        printf("输入点名人数(少于%d)：",m-i);        scanf("%d",&n);        fflush(stdin);        if(n>m-i) printf("输入错误:不能点大于超过总人数的学生\n");         else break;    }    n+=i;    srand(time(0)); //随机数种子     for(;i<n;i++)    {        t[i]=rand()%m+min;//计算学号         for(j=0,s=0;j<i;j++)            if(t[i]==t[j]) s=1;//抽取学号与已抽学号重复时开关s为真            if(s==1) {i--;continue;}//重复时，不输出学号且对i自减一次        printf("学号为%d的同学\n",t[i]);    }    if(m-i==0)    {printf("\n谢谢使用\n");exit(0);}    printf("是否继续点名? 1-yes else-no:");    if(getch()=='1') {putchar('\n'); goto loop;}    else printf("\n谢谢使用\n");} ///////////////////////////////////////////////////////////////////////V3///////////////////////////////////////////////////////////////////////////////////////////////  //由于学号并不连续,用一个数组来存储学号#include<stdio.h>#include<stdlib.h>#include<time.h>#include<conio.h>void fun(int min,int max,int l,int num[200]);void main(){    int i,j,max,min,n,t[200],s=0,num[200],l=0;//数组num存放学号    printf("版本V3.能在多段学号内随机抽取。\n分段输入学号范围：(分别按升序输入最小值和最大值，-1结束输入)\n");    for(l=0;;)    {    scanf("%d",&min);    if (min==-1) break;//判断结束输入条件    if (l!=0&&min<=max) {printf("按升序输入，不能有交集！请重新输入！\n");continue;}    //第一次循环max没有初始化，因此跳过判断    //而重复的学号会导致重复点名    scanf("%d",&max);    fflush(stdin);    if(min>max||min<0||max<0) {printf("输入错误，继续输入\n"); continue;}    fun(min,max,l,num);    l+=max-min+1;//l为fun为num加入学号后下一个元素的下标同时也是数组有效数据的数量    }i=0;loop:        while(1)    {        printf("输入点名人数(少于%d)：",l-i);        scanf("%d",&n);        fflush(stdin);        if(l-i<n) printf("输入错误:不能点超过剩余人数的学生\n");         else break;    }    n+=i;        srand(time(0));      for(;i<n;i++)    {        t[i]=rand()%l; //随机数将作为数组num的下标        for(j=0,s=0;j<i;j++)            if(t[i]==t[j]) s=1;            if(s==1) {i--;continue;}        printf("[%d]学号为%d的同学\n",i+1,num[t[i]]);//用一个数组来做另一个数组的下标    }    if(l==i)    {    printf("\n谢谢使用\n");    exit(0);}    printf("是否继续点名? 1-yes else-no:");    if(getch()=='1') {putchar('\n'); goto loop;}    else printf("\n谢谢使用\n");}void fun(int min,int max,int l,int num[200]){    int x=min,i;    for(i=0;x<=max;i++,x++)    {        num[l+i]=x;    }//将num数组的l个元素开始递增赋值到输入的最大值}        ///////////////////////////////////////////////////////////////////////V4///////////////////////////////////////////////////////////////////////////////////////////////  //使用指针使连续复制代码更简洁，改进判断多个区间是否重复而取消了升序的限制#include<stdio.h>#include<stdlib.h>#include<time.h>#include<conio.h>void fun(int min,int max,int *p);int isinc(int min,int max,int *q);void main(){    int i,j,max,min,n,t[200],s=0,num[200]={0},l=0;    int *p=&num[0],*q=&num[0];//两个指针互不干扰    printf("版本V4.能在多段学号内随机抽取。\n分段输入学号范围：(分别按输入最小值和最大值，-1结束输入)\n");    for(l=0;;)    {    scanf("%d",&min);    if (min==-1) break;//判断结束输入条件    scanf("%d",&max);    fflush(stdin);    q=&num[0];    if (l!=0&&isinc(min,max,q)==1)    {printf("数据无效：输入数据有交集，重新输入\n"); continue;}    if(min>max||min<0||max<0) {printf("数据无效，继续输入\n"); continue;}    fun(min,max,p);    l+=max-min+1;//l为fun为num加入学号后下一个元素的下标同时也是数组有效数据的数量    }i=0;loop:    while(1)    {        printf("输入点名人数(少于%d)：",l-i);        scanf("%d",&n);        fflush(stdin);        if(l-i<n) printf("输入错误:不能点超过剩余人数的学生\n");         else break;    }    n+=i;    srand(time(0));      for(;i<n;i++)    {        t[i]=rand()%l; //随机数将作为数组num的下标        for(j=0,s=0;j<i;j++)            if(t[i]==t[j]) s=1;            if(s==1) {i--;continue;}        printf("[%d]学号为%d的同学\n",i+1,*(num+*(t+i)));//用一个数组来做另一个数组的下标    }        if(l==i)    {    printf("\n谢谢使用\n");    exit(0);}    printf("是否继续点名? 1-yes else-no:");    if(getch()=='1') {putchar('\n'); goto loop;}    else printf("\n谢谢使用\n");}void fun(int min,int max,int *p){    int x=min;    for(;x<=max;x++)    {        *p++=x;//*p对应的值改变后移动到下一位    }//将num数组的l个元素开始递增赋值到输入的最大值}int isinc(int min,int max,int *q){    int i;    for(i=0;*q!=0;i++)//当*q到有效数据末端时退出    {        if(min==*q||max==*q) //输入区间端点存在于任一已有数据即为区间重复，返回1，否则返回0            return 1;        q++;    }    return 0;} /////////////////////////////////////////////////////////////////////V5///////////////////////////////////////////////////////////////////////////////////////////////// 
 //使用文件操作和复杂数据类型，简化代码，省去大量输入，并能显示姓名#include<stdio.h>#include<stdlib.h>#include<time.h>#include<string.h>#include<conio.h>#define NUM 250//最多支持学生数，大于250时应修改，否则出错struct stu{    char num[10];    char name[30];};//复杂数据类型，存储学生的学号和姓名。学号不超过十位，姓名不超过30位void main(){    int i,j,k,s=0,n=0,t[NUM];        FILE *fp;    struct stu stu1[NUM]={0};    printf("版本V5.能在所有学号内随机抽取。当前支持%d个学生,超出请修改NUM值\n",NUM);    if((fp=fopen("list.txt","r"))==NULL)    {        printf("文件打开失败,请检查目录下是否有list.txt!\n");        exit(0);    }    else printf("打开成功\n");    for(;(fgetc(fp)!='\n'););//跳过第一行        for(i=0;;i++)    {        fscanf(fp,"%s%s",&stu1[i].num,&stu1[i].name);        if(strlen(stu1[i].num)<=2)//判断空行,为空时退出            break;        }    j=0;loop:    while(1)    {        printf("输入点名人数(少于%d)：",i-j);        scanf("%d",&n);        fflush(stdin);        if(i<n) printf("输入错误:不能点超过剩余人数%d的学生\n",i-j);         else break;    }    n+=j;    srand(time(0));     for(;j<n;j++)    {        t[j]=rand()%i;        for(k=0,s=0;k<j;k++)            if(t[j]==t[k]) s=1;            if(s==1) {j--;continue;}        printf("[%d]学号为%s,姓名为%s的同学\n",j+1,stu1[t[j]].num,stu1[t[j]].name);    }    if(j==i)    {    printf("\n谢谢使用\n");    exit(0);}    printf("是否继续点名? 1-yes else-no:");    if(getch()=='1') {putchar('\n'); goto loop;}    else printf("\n谢谢使用\n");}好像是还有点问题的。
 
#include <stdio.h>
#include<time.h>
#include<stdlib.h>
 
void main ()//结构体定义： struct student { char num[20];  char name[10];  int time; } std[SIZE];  //将fp所指向的文件中的数据存储到数组std中 FILE *fp;   if((fp=fopen("myfile.dat","ab+"))==NULL)  {   printf("文件打开失败\n");  exit(0); }   for(i=0;i<SIZE;i++)   fread(&std[i],sizeof(struct student),1,fp); //调用rand的函数进行点名 loop: j=rand()/1000;  if(j==0)  {   std[0].time++;     printf("\t\t%s\t\t%s\n",std[j].num,std[j].name);  }    else if(j==1)  {    std[1].time++;   printf("\t\t%s\t\t%s\n",std[j].num,std[j].name);   }    else if(j==2)  {  std[2].time++;  printf("\t\t%s\t\t%s\n",std[j].num,std[j].name); }  else if(j==3)   {   std[3].time++;   printf("\t\t%s\t\t%s\n",std[j].num,std[j].name); }      else goto loop;  break;}  }   fclose(fp); }  //保存点名 void save() {int i; FILE *fp;  if((fp=fopen("myfile.dat","rb+"))==NULL) {printf("文件打开失败\n");   exit(0);}    for(i=0;i<SIZE;i++)    if(fwrite(&std[i],sizeof(struct student),1,fp)!=1)  printf("文件保存失败\n"); }    V3:#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<conio.h>
#include<time.h>
#define NUM_std  3

int fun(int a[],char b[NUM_std][10],int n);      //声明带参数有int类型返回值的函数fun，将数组a,b和n作为形参

int d[NUM_std];
char absence[NUM_std][10];        //将数组d,absence定义为全局变量，在主函数和自定义fun函数中均能使用

void main()
{
	int i,n=0,a[NUM_std]={0},L;
	char b[NUM_std][10]={0};
	printf("please input the students' numbers in sequential order:\n");     //依次输入学生的学号
	for(i=0;i<NUM_std;i++)
		scanf("%d",&a[i]);
	for(i=0;i<NUM_std;i++)
	{      
		printf("input the name of the %dth student: ",i+1);      //输入学生的姓名

		fflush(stdin);          //清除键盘缓存区
		gets(b[i]);
	}
	printf("please have a roll-call randomly now!\n");
	getchar();

	srand((int)time(NULL));

	L=fun(a,b,n);               //调用函数fun，并将函数fun返回值赋给L
	if(L==0)
		printf("\nAll the students you have called are here,congratulations!\n\n");     //缺席人数为0
	else
	{
		printf("The absent students are as followed:\n");
		for(i=0;i<L;i++)
			printf("%d,%s   ",d[i],absence[i]);          //输出缺席的学生的学号和姓名
		printf("\n\n");
	}
} 

int fun(int a[],char b[][10],int n)
{
	int i,j=0,flag=0,c[NUM_std]={0};    //将数组c中的元素全部初始化为0
	char reply1,reply2;
	do
	{
		i=rand()%NUM_std+0;          //产生随机数

		if(c[i]==1) continue;      //如果产生的随机数i对应的数值c中的元素为1，说明随机数重复，则重新产生随机数

		c[i]=1;          //将产生过的随机数i对应的数值c中的元素赋值为1

		n++;      //点名的次数加1

		printf("\nthe %dth lucky student is:%d,%s.\n\nIs he/she here(Y/y or N/n)?",n,a[i],b[i]);
		reply1=getche();
		if(reply1=='n'||reply1=='N')
		{
			d[j]=a[i];
			strcpy(absence[j],b[i]);    //如果被点名的同学不在，则把他/她的姓名和学号分别存入数组d,absence

			j++;   //不在的人数加1
		} 
		if(n==NUM_std)
		{  
			flag=1;
			printf("\n\nAll the students are called,thanks for using!\n");     //点名的次数等于全部人数，则退出循环
			break;
		}
		printf("\n\nDo you want to call another student(Y/y or N/n)?\n");
		fflush(stdin);
		reply2=getche();
		printf("\n");
	}
while(reply2=='y'||reply2=='Y');        //可根据操作者的回答控制循环是否结束

if(!flag)
{   
   printf("\n\nthe roll-call is over,thanks for using!\n");     
   printf("\nYou have called %d students.\n",n);       //点名的次数不等于全部人数，则提示点了多少次
}
 return(j);          //返回缺席学生人数j
} 
V5：#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<conio.h>
#include<time.h>
#include<memory.h>
#define NUM_std  3           

struct student_info            //定义复杂数据类型，用于储存学生的学号和姓名
{
	int no;
	char name[10];
}stu1[NUM_std]={0},stu2[NUM_std]={0};    //定义结构体类型数组stu1,stu2并清0

typedef struct student_info STUINFO;     //为结构体类型起别名,使程序更加简洁
   
void main()
{  
	int i,n=0,j=0,flag=0,c[NUM_std]={0};    //将数组c中的元素全部初始化为0
	char reply1,reply2;

    FILE *fp;       //定义文件类型指针fp
	

	printf("please input the students' numbers and names in sequential order:\n");     
	for(i=0;i<NUM_std;i++)
		scanf("%d,%s",&stu1[i].no,stu1[i].name);    //将全部学生的学号和姓名储存到结构体类型数组stu1中
	
    fp=fopen("student.dat","wt");      //以读写方式打开文本文件student.dat
	if(fp==NULL)
	{
		printf("can't create file:student.dat\n");     //打开失败，则退出系统
		exit(0);               
	}
	fwrite(stu1,sizeof(STUINFO),NUM_std,fp);          //将全部学生信息写入文件fp中

	rewind(fp);            //将文件位置指针指回到文件头

    memset(stu1,0,NUM_std*sizeof(STUINFO));           //清除学生信息

	printf("please have a roll-call randomly now!\n");
	
    srand((int)time(NULL));
             
    do
	{
		fp=fopen("student.dat","rt");      //以只读方式打开student.dat文件
	    if(fp==NULL)
		{
		printf("can't open file:student.dat\n");        //打开失败
		exit(0);
		}
		rewind(fp);
		i=rand()%NUM_std+0;     //产生随机数

        if(c[i]==1) continue;    //如果以产生的随机数i为下标的数组c中的元素为1，说明随机数重复，则重新产生随机数
		
		c[i]=1;       //将以产生过的随机数i为下标的数组c中的元素赋值为1

        n++;         //点名的次数加1
		
		fseek(fp,i*sizeof(STUINFO),SEEK_SET);     //文件位置指针定位

		fread(&stu1[i],sizeof(STUINFO),1,fp);       //读取一个学生的信息

		printf("\nthe %dth lucky student is:%d,%s.\n\nIs he/she here(Y/y or N/n)?",n,stu1[i].no,stu1[i].name);      //输出被点学生的学号和姓名
        reply1=getche();
		if(reply1=='n'||reply1=='N')
		{
			stu2[j].no=stu1[i].no;            
            strcpy(stu2[j].name,stu1[i].name);    //结构体类型数组stu2中储存缺席学生的学号和姓名
			j++;   
		} 
		fclose(fp);          //关闭文件
		if(n==NUM_std)
		{  
			flag=1;
			printf("\n\nAll the students are called,thanks for using!\n");     //点名的次数n等于全部人数，则退出循环
			break;
		}
		printf("\n\nDo you want to call another student(Y/y or N/n)?\n");
		reply2=getche();
		printf("\n");
	}
while(reply2=='y'||reply2=='Y');   //可根据操作者的回答控制循环是否结束     

if(!flag)  
   printf("\n\nthe roll-call is over,thanks for using!\n");        

if(j==0)
   printf("\nAll the %d students you have called are here,congratulations!\n\n",n);    //缺席人数为0时显示点名的次数
else
{
   printf("\nYou have called %d students.\n",n);
   printf("The %d absent students are as followed:\n",j);
   for(i=0;i<j;i++)
	  printf("%d,%s   ",stu2[i].no,stu2[i].name);          //输出缺席的学生的学号和姓名
   printf("\n\n");
}

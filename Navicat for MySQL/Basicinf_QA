1483;2189;1015080310150803;https://c.rin.to/course/mod/forum/discuss.php?d=2189;求方程的根，程序运行就崩溃了;#include <stdio.h>#include <math.h>void main(){float a,b,c,d,x1,x2;printf("请输入a,b,c:");scanf("%f%f%f,&a,&b,&c");d=(b*b-4*a*c);if (d>=0){ if(d=0) {x1=(-b)/a*2;          printf("x1=%.2f\n",x1); }    elsex1=((-b)+sqrt(d))/a*2;x2=((-b)-sqrt(d))/a*2;printf("x1=%.2f\n x2=%.2f\n",x1,x2);  }elseprintf("方程没有实根\n");}哪里有问题啊= = 谢谢大神们; if(d=0) {x1=(-b)/a*2;觉得应该改x1=(-b)/(a*2);不过可能还是得不到结果#include <stdio.h>
#include <math.h>
void main()
{
  float a,b,c,d,x1,x2;
  printf("请输入a,b,c:");
  scanf("%f%f%f",&a,&b,&c);
  d=(b*b-4*a*c);
  if (d>=0)
  {
    if(d==0)
    {
      x1=(-b)/(a*2);
      printf("x1=%.2f\n",x1);
    }
    else
    {
      x1=((-b)+sqrt(d))/(a*2);
      x2=((-b)-sqrt(d))/(a*2);
      printf("x1=%.2f\n x2=%.2f\n",x1,x2);  
    }
  }
  else
    printf("方程没有实根\n");
}

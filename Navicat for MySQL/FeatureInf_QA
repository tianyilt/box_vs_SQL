void Sort(int a[],int n){
    int i;
    int j;
    int temp = 0;
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
        if (a[j]<a[j+1])
            temp = a[j];
            a[j] = a[j+1];
            a[j+1] = temp;
        }
    }
}


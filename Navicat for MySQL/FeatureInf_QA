983;1336;尝试极端情况模拟函数运行inf *temp;temp = point(head, b)->next;假设point(head, b)返回的是整个链表的第1项，此时temp指向第二项。point(head, b)->next = point(head, a)->next;假设point(head, a)返回整个链表的最后一项，那么现在链表第一项中记录的就是没有下一项。point(head, a)->next = temp;此时链表已经被修改过，整个链表只有一项，假定a大于1，point将无法找到第a项，更别提修改节点的下一项指向原来的第二项了。void exchange(inf **head, int a, int b) {  inf *lpAprev, lpBprev, temp;  lpAprev = point(head, a-1);  lpBprev = point(head, b-1);  temp = lpAprev->next;  lpAprev->next = lpBprev->next;  lpBprev->next = temp;  temp = lpBprev->next->next;  lpBprev->next->next = lpAprev->next->next;  lpAprev->next->next = temp;}#include<stdio.h>#include<malloc.h>struct inf{    int no;    char name[8];    int chinese;    int math;    int    english;    int sum;    float average;    inf *next;};inf *Create_LinkList();inf *point(inf *head,int i);void exchange(inf *head,int a,int b);void main(){    int n,i,x,y;    inf *head;    printf("please key in the number of students:");    scanf_s("%d", &n);    head = Create_LinkList();    for (x = 1; x < n; x++)    {        for (y = 1; y <= n-x;y++)        {            if (point(head, y)->sum < point(head, y+1)->sum)                exchange(head, x, y);        }    }    printf("no    name    chinese        math        english        sum        average\n");    printf("--------------------------------------------------------------------------------\n");    for (i = 1; i <= n; i++)        printf("%d    %s    %d        %d        %d        %d        %.2f\n", point(head, i)->no, point(head, i)->name, point(head, i)->chinese, point(head, i)->math, point(head, i)->english, point(head, i)->sum, point(head, i)->average);}inf *Create_LinkList()//链表生成函数{    inf *head, *tail, *pnew;    int no,chinese,math,english,i;    char name[8];    head = (inf*)malloc(sizeof(inf));    if (head == NULL)    {        printf("no enough memory!\n");        return(NULL);    }    head->next = NULL;    tail = head;    while (1)    {        printf("key in no:");        scanf_s("%d", &no);        if (no < 0)        break;        fflush(stdin);        printf("key in name:");        gets_s(name);        printf("key in chinese score:");        scanf_s("%d", &chinese);        printf("key in math score:");        scanf_s("%d", &math);        printf("key in english score:");        scanf_s("%d", &english);        pnew = (inf*)malloc(sizeof(inf));        if (pnew == NULL)        {            printf("no enough memory!\n");            return(NULL);        }        pnew->no = no;        for (i = 0; i < 8; i++)            pnew->name[i] = name[i];        pnew->chinese = chinese;        pnew->math = math;        pnew->english = english;        pnew->sum = chinese + math + english;        pnew->average = (float)(chinese + math + english) / 3;        pnew->next = NULL;        tail->next = pnew;        tail = pnew;    }    return(head);}inf *point(inf *head,int i)  //默认第0个节点是头结点,该函数的作用是调用第i个节点,并返回第i个节点的地址{    inf *a,*search;    int n;    a = head;    search = head;    for (n = 1; n <= i; n++)    {        search = a->next;        a = search;    }    return(search);}void exchange(inf *head,int a, int b)//该函数的目的是交换第a个节点和第b个节点的地址{    inf *temp;    temp = point(head, b)->next;    point(head, b)->next = point(head, a)->next;    point(head, a)->next = temp;    temp = point(head, b);    point(head, b-1)->next = point(head, a);    point(head, a - 1)->next = temp;};2798;1;584;11;1;0

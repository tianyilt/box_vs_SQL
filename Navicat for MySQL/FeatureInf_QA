1838;3108;#define NUM 10#include<stdio.h>#include<stdlib.h>#include<windows.h>int InsertSearch(int a[],int value,int low,int high){ int mid; while(low<=high) { mid=low+((value-a[low])/(a[high]-a[low]))*(high-low); if(value==a[mid]) { return mid; } else if(value>a[mid]) { return InsertSearch(a,value,mid+1,high); } else if(value<a[mid]) { return InsertSearch(a,value,low,mid-1); } } return -1;}int main(){ int i,j,a,score[NUM],x,y,n,k; FILE *fp; fp=fopen("score.txt","r"); if(fp==NULL) { puts("can't open score.txt"); exit(0); } do { printf("假装正在打开文件\n"); for(n=0;n<5;n++) { for(k=0;k<(n+1);k++) { printf("▇"); } for(k=0;k<(5-(n+1));k++) { printf("  "); } printf("%%%d",(n+1)*20); Sleep(1000); for(k=0;k<20;k++) { printf("\b"); } } }while(0); system("cls"); i=0; printf("原始成绩数据如下:\n"); while(!feof(fp)) { fscanf(fp,"%d",&score[i]); printf("%2d ",score[i]); i++; } for(i=0;i<NUM-1;i++) for(j=0;j<NUM-1-i;j++) if(score[j]>score[j+1]) { a=score[j+1]; score[j+1]=score[j]; score[j]=a; } printf("\n排序后的成绩数据为:\n"); for(i=0;i<NUM;i++) { printf("%d ",score[i]); } printf("\n输入要查找的学生成绩数据:\n"); scanf("%d",&x); y=InsertSearch(score,x,0,NUM-1); if(y==-1) printf("%d成绩没有找到!\n",x); else printf("%d成绩找到,下标为%d的位置上\n",x,y); system("pause"); return 0;}#define N 10#include<stdio.h>#include<stdlib.h>int BinSearch(int a[],int x){ int low=0,high=N-1,mid; while(low<=high) { mid=(low+high)/2; if(x==a[mid]) return mid; else if(x>a[mid]) low=mid+1; else high=mid-1; } return -1;}int main(){ int i,j,a,score[N],x,y; FILE *fp; fp=fopen("score.txt","r"); if(fp==NULL) { puts("can't open score.txt"); exit(0); } i=0; printf("原始成绩数据如下:\n"); while(!feof(fp)) { fscanf(fp,"%d",&score[i]); printf("%2d ",score[i]); i++; } for(i=0;i<N-1;i++) for(j=0;j<N-1-i;j++) if(score[j]>score[j+1]) { a=score[j+1]; score[j+1]=score[j]; score[j]=a; }  printf("\n排序后的成绩数据为:\n"); for(i=0;i<N;i++) { printf("%d ",score[i]); } printf("\n输入要查找的学生成绩数据:\n"); scanf("%d",&x); y=BinSearch(score,x); if(y==-1) printf("%d成绩没有找到!\n",x); else printf("%d成绩找
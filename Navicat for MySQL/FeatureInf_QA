void sort(int *x,int *y) 
  
{ int temp; 
  
  if (*x>*y) 
  
  { temp=*x; 
  
    *x=*y; 
  
 *y=temp; 
  
  } 
  
} 
  
main() 
  
{  

   int a[N],i,j; 
  
   printf("成绩排序，请输入10位同学的成绩：\n"); 
  
   { 
  
   for(i=0;i<N;i++) 
  
   scanf("%d",&a[i]); 
  
                    } 
  
   for(i=0;i<N-1;i++) 
  
